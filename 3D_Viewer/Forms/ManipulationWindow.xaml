<Window x:Class="smileUp.Forms.ManipulationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ManipulationWindow" Height="300" Width="300">
    <Grid>
        <Button Content="1. Select Patient" Height="23" HorizontalAlignment="Left" Margin="12,51,0,0" Name="button2" VerticalAlignment="Top" Width="97" Click="button2_Click" />
        <Button Content="2. Create Treatment" Height="23" HorizontalAlignment="Left" Margin="128,51,0,0" Name="button1" VerticalAlignment="Top" Width="116" />
        <TextBox Height="76" HorizontalAlignment="Left" Margin="12,111,0,0" Name="textBox1" VerticalAlignment="Top" Width="232" />
        <TextBlock Height="16" HorizontalAlignment="Left" Margin="213,89,0,0" Name="textBlock1" Text="Notes" VerticalAlignment="Top" />
        <Button Content="3. Save Progress" Height="23" HorizontalAlignment="Left" Margin="45,203,0,0" Name="button3" VerticalAlignment="Top" Width="109" Click="button3_Click" />
        <Button Content="4. Finish" Height="23" HorizontalAlignment="Left" Margin="169,203,0,0" Name="button4" VerticalAlignment="Top" Width="75" Click="button4_Click" />
    </Grid>
</Window>
