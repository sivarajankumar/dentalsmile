<Window x:Class="smileUp.Forms.AdminPasswordDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AdminPasswordDialog" Height="300" Width="300">
    <Grid>
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="58,119,0,0" Name="txtPasswd" VerticalAlignment="Top" Width="208" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,90,0,0" Name="textBlock1" Text="Administrator Password:" VerticalAlignment="Top" Width="254" />
        <Button Content="Set" Height="23" HorizontalAlignment="Left" Margin="156,226,0,0" Name="button1" VerticalAlignment="Top" Width="110" Click="button1_Click" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="58,185,0,0" Name="txtConfirmPasswd" VerticalAlignment="Top" Width="208" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,156,0,0" Name="textBlock2" Text="Confirm Password:" VerticalAlignment="Top" Width="254" />
    </Grid>
</Window>
