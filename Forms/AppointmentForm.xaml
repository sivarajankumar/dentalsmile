<Window x:Class="smileUp.Forms.AppointmentForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:cal="clr-namespace:smileUp.Calendar"
        Title="Appointment Form" Height="500" Width="500" WindowStartupLocation="CenterScreen">
    <cal:MonthView x:Name="AptCalendar" VerticalAlignment="Stretch" VerticalContentAlignment="Stretch" DisplayMonthChanged="AptCalendar_DisplayMonthChanged" DayBoxDoubleClicked="AptCalendar_DayBoxDoubleClicked" AppointmentDblClicked="AptCalendar_AppointmentDblClicked"/>
</Window>
