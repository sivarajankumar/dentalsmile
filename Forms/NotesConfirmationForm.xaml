<Window x:Class="smileUp.Forms.NotesConfirmationForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Notes Confirmation Dialog" Height="331" Width="300">
    <Grid>
        <TextBox Height="61" HorizontalAlignment="Left" Margin="24,172,0,0" Name="txtNotes" VerticalAlignment="Top" Width="242" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="24,143,0,0" Name="textBlock1" Text="File Description:" VerticalAlignment="Top" />
        <Button Content="Save" Height="23" HorizontalAlignment="Left" Margin="191,259,0,0" Name="btnOK" VerticalAlignment="Top" Width="75" Click="btnOK_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="95,259,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
        <TextBox Height="61" HorizontalAlignment="Left" Margin="24,76,0,0" Name="txtResumeMedic" VerticalAlignment="Top" Width="242" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="24,47,0,0" Name="textBlock2" Text="Resume Medic:" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="24,12,0,0" Name="textBlock3" Text="Do you want to add some notes for history?" VerticalAlignment="Top" Width="242" />
    </Grid>
</Window>
